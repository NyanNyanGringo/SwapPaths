[HKEY_CLASSES_ROOT\Directory\Background\shell\SwapPaths]
@="Swap Paths"
"Icon"="explorer.exe"
"NoWorkingDirectory"=""

[HKEY_CLASSES_ROOT\Directory\Background\shell\SwapPaths\command]
